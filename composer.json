{
    "name": "shaunluedeke/livewire-ace",
    "description": "Ace editor for livewire",
    "version": "0.0.2",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Osman Mesut Ozcan",
            "email": "osmanmesutozcan@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1|^8.2|^8.3",
        "livewire/livewire": "^3",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.5|^9.1",
        "portavice/laravel-pint-config": "v2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Panikka\\LivewireAce\\": "src/"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Panikka\\LivewireAce\\LivewireAceServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
